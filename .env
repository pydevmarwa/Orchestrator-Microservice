# Copy to .env and edit if needed (do NOT commit .env with secrets)
KAFKA_BOOTSTRAP=kafka:9092
REDIS_HOST=redis
REDIS_PORT=6379
#OBJECTS_PATH=/app/tools/objects.json
OBJECTS_PATH=/app/tools/objects_complete.json

# Topics
TOPIC_READY=event.load.ready
TOPIC_LOADED=event.load.loaded
TOPIC_FAILED=event.load.failed
TOPIC_DLQ=event.load.dlq

# Worker tuning
MAX_WORKERS=4
MAX_RETRIES=2
RETRY_TTL_SECONDS=3600

# Worker zones and process durations (defaults)
WORKER_1_PROC_SEC=20
WORKER_2_PROC_SEC=10
WORKER_3_PROC_SEC=5

#Logging
LOG_FILE=app.log
LOG_LEVEL=INFO
